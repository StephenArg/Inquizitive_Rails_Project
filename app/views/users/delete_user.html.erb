<h1>Select a User to Delete</h1>



<%=form_for @user, url: multi_delete_user_path, method: :delete do |f|%>
    <%=f.collection_check_boxes :id, @users, :id, :name_email do |b|%>
        <%= b.check_box(class: "check_box") %>
        <%= b.object.name_email %>
        <br/>
    <% end %>
    <%=f.submit "Delete Users"%>
<%end%> 